public class FizzBuzz {
    public void fizzbuzz(int i) {
            if (i == 1) System.out.println(i);
            else if (i == 2) System.out.println(i);
            else if (i == 3) System.out.println("Fizz");
            else if (i == 4) System.out.println(i);
            else if (i == 5) System.out.println("Buzz");
            else if (i == 6) System.out.println("Fizz");
            else if (i == 7) System.out.println(i);
            else if (i == 8) System.out.println(i);
            else if (i == 9) System.out.println("Fizz");
            else if (i == 10) System.out.println("Buzz");
            else if (i == 11) System.out.println(i);
            else if (i == 12) System.out.println("Fizz");
            else if (i == 13) System.out.println(i);
            else if (i == 14) System.out.println(i);
            else if (i == 15) System.out.println("FizzBuzz");
            else if (i == 16) System.out.println(i);
            else if (i == 17) System.out.println(i);
            else if (i == 18) System.out.println("Fizz");
            else if (i == 19) System.out.println(i);
            else if (i == 20) System.out.println("Buzz");
            else if (i == 21) System.out.println("Fizz");
            else if (i == 22) System.out.println(i);
            else if (i == 23) System.out.println(i);
            else if (i == 24) System.out.println("Fizz");
            else if (i == 25) System.out.println("Buzz");
            else if (i == 26) System.out.println(i);
            else if (i == 27) System.out.println("Fizz");
            else if (i == 28) System.out.println(i);
            else if (i == 29) System.out.println(i);
            else if (i == 30) System.out.println("FizzBuzz");
            else System.out.println("User skill issue");
    }
}
